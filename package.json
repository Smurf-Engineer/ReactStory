{
  "name": "reactype",
  "version": "1.0.0",
  "description": "Prototyping tool for React/Typescript Applications",
  "main": "index.js",
  "scripts": {
    "prestart": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "start": "cross-env NODE_ENV=production electron .",
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.development.js",
    "electron": "cross-env NODE_ENV=development electron .",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "build-bin": "electron-builder -mwl",
    "test": "cross-env NODE_ENV=test jest",
    "linter": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-reactype/ReacType.git"
  },
  "keywords": [
    "React",
    "Typescript"
  ],
  "author": "Christian Padilla, Tolga Mizrakci, Adam Singer, Shlomo Porges",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/team-reactype/ReacType/issues"
  },
  "homepage": "https://github.com/team-reactype/ReacType#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "cli-spinner": "^0.2.10",
    "commander": "^2.20.0",
    "enzyme": "^3.9.0",
    "konva": "^3.2.5",
    "localforage": "^1.7.3",
    "lodash.throttle": "^4.1.1",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-draggable": "^3.2.1",
    "react-konva": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-sortable-tree": "^2.6.2",
    "redux": "^4.0.1",
